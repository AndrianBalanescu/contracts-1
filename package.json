{
  "name": "vanilla-contracts",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "npm run compile:sol && npx hardhat test",
    "format:js": "eslint . --fix",
    "format:sol": "prettier --write contracts/",
    "lint:js": "eslint .",
    "lint:sol": "prettier --check contracts/ && solhint --max-warnings 0 'contracts/**/*.sol'",
    "compile:sol": "cp test/contracts/TestUtils.sol contracts/ && npx hardhat compile && cp artifacts/contracts/TestUtils.sol/VanillaRouterDelegate.json test/contracts/ && rm contracts/TestUtils.sol",
    "node:mainnet-fork": "npx hardhat clean && FORK=mainnet npx hardhat node --fork-deployments mainnet --as-network localhost",
    "deploy:goerli": "npx hardhat clean && npx hardhat --network goerli deploy",
    "deploy:ropsten": "npx hardhat clean && npx hardhat --network ropsten deploy",
    "node:test-accounts": "npx hardhat test-accounts"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@ethereum-waffle/chai": "^3.2.2",
    "@nomiclabs/hardhat-ethers": "npm:hardhat-deploy-ethers@^0.3.0-beta.7",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@openzeppelin/cli": "^2.8.2",
    "@openzeppelin/upgrades-core": "^1.2.0",
    "@types/chai": "^4.2.14",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.14.3",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "chai": "^4.2.0",
    "eslint": "^7.12.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.2",
    "ethereum-waffle": "^3.1.2",
    "ethers": "^5.0.19",
    "fast-check": "^2.7.0",
    "hardhat": "^2.1.1",
    "hardhat-deploy": "^0.7.0-beta.48",
    "hardhat-deploy-ethers": "^0.3.0-beta.7",
    "prettier": "^2.1.2",
    "prettier-plugin-solidity": "^1.0.0-alpha.59",
    "solc": "^0.7.4",
    "solhint": "^3.2.2",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3",
    "dotenv": "^8.2.0"
  },
  "dependencies": {
    "@openzeppelin/contracts": "3.3.0",
    "@uniswap/v2-core": "1.0.1",
    "@uniswap/v2-periphery": "1.1.0-beta.0",
    "graphql": "15.5.0",
    "graphql-request": "3.4.0"
  }
}
